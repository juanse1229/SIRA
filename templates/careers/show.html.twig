{% extends 'base.html.twig' %}

{% block title %}Careers{% endblock %}

{% block body %}
    <h1>Careers</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ career.id }}</td>
            </tr>
            <tr>
                <th>CareerName</th>
                <td>{{ career.careerName }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ career.createdAt ? career.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ career.updatedAt ? career.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_careers_index') }}">back to list</a>

    <a href="{{ path('app_careers_edit', {'id': career.id}) }}">edit</a>

    {{ include('careers/_delete_form.html.twig') }}
{% endblock %}
